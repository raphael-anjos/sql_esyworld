SELECT *  FROM fn_titulo_receber  WHERE cod_empresa = '1' AND  cod_transacao = '14'
 SELECT fn_titulo_receber.cod_transacao  FROM fn_titulo_receber_baixa, fn_titulo_receber_hist, fn_titulo_receber  WHERE fn_titulo_receber_baixa.cod_transacao = 14 AND  fn_titulo_receber_hist.cod_baixa_receber = fn_titulo_receber_baixa.cod_baixa_receber AND  fn_titulo_receber_hist.cod_empresa = fn_titulo_receber_baixa.cod_empresa AND  fn_titulo_receber.cod_empresa = fn_titulo_receber_hist.cod_empresa AND  fn_titulo_receber.cod_titulo = fn_titulo_receber_hist.cod_titulo AND  fn_titulo_receber.cod_parcela = fn_titulo_receber_hist.cod_parcela AND  fn_titulo_receber.serie = fn_titulo_receber_hist.serie AND  fn_titulo_receber.cod_transacao is not null
 SELECT fn_titulo_receber.cod_transacao  FROM fn_titulo_receber_baixa, fn_titulo_receber_hist, fn_titulo_receber  WHERE fn_titulo_receber_baixa.cod_transacao = 10 AND  fn_titulo_receber_hist.cod_baixa_receber = fn_titulo_receber_baixa.cod_baixa_receber AND  fn_titulo_receber_hist.cod_empresa = fn_titulo_receber_baixa.cod_empresa AND  fn_titulo_receber.cod_empresa = fn_titulo_receber_hist.cod_empresa AND  fn_titulo_receber.cod_titulo = fn_titulo_receber_hist.cod_titulo AND  fn_titulo_receber.cod_parcela = fn_titulo_receber_hist.cod_parcela AND  fn_titulo_receber.serie = fn_titulo_receber_hist.serie AND  fn_titulo_receber.cod_transacao is not null
 SELECT fn_titulo_receber.cod_transacao  FROM fn_titulo_receber_baixa, fn_titulo_receber_hist, fn_titulo_receber  WHERE fn_titulo_receber_baixa.cod_transacao = 10 AND  fn_titulo_receber_hist.cod_baixa_receber = fn_titulo_receber_baixa.cod_baixa_receber AND  fn_titulo_receber_hist.cod_empresa = fn_titulo_receber_baixa.cod_empresa AND  fn_titulo_receber.cod_empresa = fn_titulo_receber_hist.cod_empresa AND  fn_titulo_receber.cod_titulo = fn_titulo_receber_hist.cod_titulo AND  fn_titulo_receber.cod_parcela = fn_titulo_receber_hist.cod_parcela AND  fn_titulo_receber.serie = fn_titulo_receber_hist.serie AND  fn_titulo_receber.cod_transacao is not null
SELECT vd_pedido.cod_cliente,  vd_pedido_comissao.cod_colaborador, vd_pedido.cod_pedido_pda,  fn_titulo_receber.cod_parcela, vd_pedido.cod_pedido,  fn_titulo_receber.cod_titulo,  fn_titulo_receber.obs_titulo,  fn_titulo_receber.cod_forma_pagto,  fn_titulo_receber.dt_vencimento, vd_nota_fiscal.cod_nota_fiscal, vd_nota_fiscal.nr_nota_fiscal, vd_nota_fiscal.dt_emissao, vd_nota_fiscal.vl_cotacao, vd_nota_fiscal.vl_total_icms,( ( vd_nota_fiscal.vl_frete + vd_nota_fiscal.vl_total_ipi ) / vd_nota_fiscal.vl_total_nota_fiscal ) as perc_reducao_ipi_frete, fn_titulo_receber.vl_total_titulo, fn_titulo_receber.vl_total_baixa, fn_titulo_receber.cod_transacao, fn_titulo_receber.ind_agrupado, fn_titulo_receber.situacao, fn_titulo_receber_hist.dt_referencia, fn_titulo_receber_hist.serie, fn_titulo_receber_hist.nr_sequencia, vd_pedido.nome_cliente,  sg_colaborador.id_usuario,  sg_colaborador.nome_colaborador,  revenda.nome_colaborador,  ( fn_titulo_receber_hist.vl_total ) as vl_total, ( fn_titulo_receber_hist.vl_baixa ) as vl_baixa,  vd_pedido.dt_implant,  vd_pedido.nop,  vd_pedido.vl_total_pedido, vd_pedido.nr_ped_compra_cli, vd_pedido_comissao.tp_owner,  vd_pedido_comissao.perc_comissao, vd_pedido_comissao.tp_comissao,  vd_pedido_nota_fiscal.perc_pedido_nf,  vd_nota_fiscal.vl_frete,  vd_nota_fiscal.vl_total_nota_fiscal,  vd_pedido.ccusto as centro_custo,  fn_centro_custo.desc_abrev as desc_centro_custo,  vd_pedido.cod_cond_pagto  FROM sg_colaborador, fn_titulo_receber LEFT JOIN fn_titulo_receber_hist ON (fn_titulo_receber_hist.cod_empresa = fn_titulo_receber.cod_empresa AND fn_titulo_receber_hist.cod_titulo = fn_titulo_receber.cod_titulo  AND fn_titulo_receber_hist.cod_parcela = fn_titulo_receber.cod_parcela  AND fn_titulo_receber_hist.serie = fn_titulo_receber.serie  AND fn_titulo_receber_hist.tp_hist = 31 AND fn_titulo_receber_hist.dt_referencia >= '2015-10-01' AND fn_titulo_receber_hist.dt_referencia <= '2016-06-20') LEFT JOIN vd_nota_fiscal ON ( vd_nota_fiscal.cod_nota_fiscal = fn_titulo_receber.cod_nota_fiscal  AND vd_nota_fiscal.cod_empresa = fn_titulo_receber.cod_empresa )  LEFT JOIN vd_pedido_nota_fiscal ON ( vd_pedido_nota_fiscal.cod_nota_fiscal = fn_titulo_receber.cod_nota_fiscal  AND vd_pedido_nota_fiscal.cod_empresa = fn_titulo_receber.cod_empresa ), vd_pedido  LEFT JOIN sg_colaborador revenda ON ( revenda.cod_colaborador = vd_pedido.cod_revenda ) LEFT JOIN fn_centro_custo ON ( fn_centro_custo.ccusto = vd_pedido.ccusto ), vd_pedido_comissao , fn_titulo_receber_baixa  WHERE fn_titulo_receber.cod_empresa = 1 AND vd_pedido_comissao.cod_pedido = vd_pedido.cod_pedido  AND vd_pedido_comissao.cod_empresa = vd_pedido.cod_empresa  AND vd_pedido_comissao.tp_owner = '2' AND vd_pedido_comissao.cod_colaborador IN (4175) AND vd_pedido.cod_pedido = fn_titulo_receber.cod_pedido  AND vd_pedido.cod_empresa = fn_titulo_receber.cod_empresa  AND vd_pedido_comissao.cod_colaborador = sg_colaborador.cod_colaborador  AND vd_pedido.ind_estatistica_venda = 1  AND fn_titulo_receber_baixa.cod_transacao IN ( 14, 10, 10 )  AND fn_titulo_receber_hist.cod_baixa_receber = fn_titulo_receber_baixa.cod_baixa_receber   AND fn_titulo_receber_hist.cod_empresa = fn_titulo_receber_baixa.cod_empresa  ORDER BY fn_titulo_receber.dt_vencimento